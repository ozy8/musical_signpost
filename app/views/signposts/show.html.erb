<p id="notice"><%= notice %></p>


<h1><%= @signpost.label %></h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <div class="jumbotron clearfix" >
      <div class="row">
        <% @musics.each do |music| %>
            <ul>
              <div>
                <%= link_to music.title, music %>
              </div>

            </ul>
        <% end %>
      </div>
    </div>
  </tbody>
</table>


<!-- <%= link_to 'Edit', edit_signpost_path(@signpost) %> | -->
<div class="bottom_links">
  <%= link_to 'Back', signposts_path %>
</div>
